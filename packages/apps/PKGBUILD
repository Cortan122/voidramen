license=('Unlicense')
pkgdesc="A dummy package with all of my aur dependencies and big apps."
pkgrel=1
pkgver=r191.a9fd359

pkgver() {
  cd "$srcdir"
  printf "r%s.%s" "$(git rev-list --count HEAD)" "$(git rev-parse --short=7 HEAD)"
}
